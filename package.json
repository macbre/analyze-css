{
  "name": "analyze-css",
  "version": "0.13.0",
  "author": "Maciej Brencz <maciej.brencz@gmail.com> (https://github.com/macbre)",
  "description": "CSS selectors complexity and performance analyzer",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/macbre/analyze-css.git"
  },
  "keywords": [
    "analysis",
    "complexity",
    "css",
    "stylesheet",
    "webperf"
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "cli": "^1.0.1",
    "commander": "^7.0.0",
    "css": "^3.0.0",
    "css-shorthand-properties": "^1.1.1",
    "debug": "^4.1.1",
    "fast-stats": "0.0.6",
    "glob": "^7.1.6",
    "http-proxy-agent": "^4.0.1",
    "node-fetch": "^2.6.0",
    "onecolor": "^3.1.0",
    "slick": "~1.12.1",
    "specificity": "^0.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "browserslist": "^4.11.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "postcss": "^8.2.6",
    "prettier": "2.3.0"
  },
  "optionalDependencies": {
    "node-sass": "5.0.0"
  },
  "bin": "./bin/analyze-css.js",
  "preferGlobal": true,
  "scripts": {
    "test": "mocha -R spec",
    "lint": "eslint .",
    "prettier": "npx prettier --write .",
    "coverage": "nyc mocha -R spec",
    "prefixes": "npx browserslist@latest --update-db; DEBUG=* node data/prefixes.js"
  },
  "jshintConfig": {
    "esversion": 6,
    "node": true,
    "strict": true,
    "validthis": true
  }
}
